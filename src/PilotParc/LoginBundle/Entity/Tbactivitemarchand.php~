<?php

namespace PilotParc\LoginBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Tbactivitemarchand
 */
class Tbactivitemarchand
{
    /**
     * @var string
     */
    private $libelleactivitemarchand;

    /**
     * @var \DateTime
     */
    private $datecreation;

    /**
     * @var \DateTime
     */
    private $datemaj;

    /**
     * @var boolean
     */
    private $idtbactivite;


    /**
     * Set libelleactivitemarchand
     *
     * @param string $libelleactivitemarchand
     * @return Tbactivitemarchand
     */
    public function setLibelleactivitemarchand($libelleactivitemarchand)
    {
        $this->libelleactivitemarchand = $libelleactivitemarchand;

        return $this;
    }

    /**
     * Get libelleactivitemarchand
     *
     * @return string 
     */
    public function getLibelleactivitemarchand()
    {
        return $this->libelleactivitemarchand;
    }

    /**
     * Set datecreation
     *
     * @param \DateTime $datecreation
     * @return Tbactivitemarchand
     */
    public function setDatecreation($datecreation)
    {
        $this->datecreation = $datecreation;

        return $this;
    }

    /**
     * Get datecreation
     *
     * @return \DateTime 
     */
    public function getDatecreation()
    {
        return $this->datecreation;
    }

    /**
     * Set datemaj
     *
     * @param \DateTime $datemaj
     * @return Tbactivitemarchand
     */
    public function setDatemaj($datemaj)
    {
        $this->datemaj = $datemaj;

        return $this;
    }

    /**
     * Get datemaj
     *
     * @return \DateTime 
     */
    public function getDatemaj()
    {
        return $this->datemaj;
    }

    /**
     * Get idtbactivite
     *
     * @return boolean 
     */
    public function getIdtbactivite()
    {
        return $this->idtbactivite;
    }
}
