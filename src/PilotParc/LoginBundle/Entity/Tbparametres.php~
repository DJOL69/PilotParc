<?php

namespace PilotParc\LoginBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Tbparametres
 */
class Tbparametres
{
    /**
     * @var string
     */
    private $libelleparametre;

    /**
     * @var string
     */
    private $valeur;

    /**
     * @var string
     */
    private $valeurpardefaut;

    /**
     * @var \DateTime
     */
    private $datecreation;

    /**
     * @var \DateTime
     */
    private $datemaj;

    /**
     * @var integer
     */
    private $idtbparametre;


    /**
     * Set libelleparametre
     *
     * @param string $libelleparametre
     * @return Tbparametres
     */
    public function setLibelleparametre($libelleparametre)
    {
        $this->libelleparametre = $libelleparametre;

        return $this;
    }

    /**
     * Get libelleparametre
     *
     * @return string 
     */
    public function getLibelleparametre()
    {
        return $this->libelleparametre;
    }

    /**
     * Set valeur
     *
     * @param string $valeur
     * @return Tbparametres
     */
    public function setValeur($valeur)
    {
        $this->valeur = $valeur;

        return $this;
    }

    /**
     * Get valeur
     *
     * @return string 
     */
    public function getValeur()
    {
        return $this->valeur;
    }

    /**
     * Set valeurpardefaut
     *
     * @param string $valeurpardefaut
     * @return Tbparametres
     */
    public function setValeurpardefaut($valeurpardefaut)
    {
        $this->valeurpardefaut = $valeurpardefaut;

        return $this;
    }

    /**
     * Get valeurpardefaut
     *
     * @return string 
     */
    public function getValeurpardefaut()
    {
        return $this->valeurpardefaut;
    }

    /**
     * Set datecreation
     *
     * @param \DateTime $datecreation
     * @return Tbparametres
     */
    public function setDatecreation($datecreation)
    {
        $this->datecreation = $datecreation;

        return $this;
    }

    /**
     * Get datecreation
     *
     * @return \DateTime 
     */
    public function getDatecreation()
    {
        return $this->datecreation;
    }

    /**
     * Set datemaj
     *
     * @param \DateTime $datemaj
     * @return Tbparametres
     */
    public function setDatemaj($datemaj)
    {
        $this->datemaj = $datemaj;

        return $this;
    }

    /**
     * Get datemaj
     *
     * @return \DateTime 
     */
    public function getDatemaj()
    {
        return $this->datemaj;
    }

    /**
     * Get idtbparametre
     *
     * @return integer 
     */
    public function getIdtbparametre()
    {
        return $this->idtbparametre;
    }
}
