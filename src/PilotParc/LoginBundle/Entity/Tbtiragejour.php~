<?php

namespace PilotParc\LoginBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Tbtiragejour
 */
class Tbtiragejour
{
    /**
     * @var integer
     */
    private $idmarchand;

    /**
     * @var integer
     */
    private $idmarche;

    /**
     * @var string
     */
    private $jour;

    /**
     * @var \DateTime
     */
    private $datecreation;

    /**
     * @var \DateTime
     */
    private $datemodification;

    /**
     * @var integer
     */
    private $idtbtiragejour;


    /**
     * Set idmarchand
     *
     * @param integer $idmarchand
     * @return Tbtiragejour
     */
    public function setIdmarchand($idmarchand)
    {
        $this->idmarchand = $idmarchand;

        return $this;
    }

    /**
     * Get idmarchand
     *
     * @return integer 
     */
    public function getIdmarchand()
    {
        return $this->idmarchand;
    }

    /**
     * Set idmarche
     *
     * @param integer $idmarche
     * @return Tbtiragejour
     */
    public function setIdmarche($idmarche)
    {
        $this->idmarche = $idmarche;

        return $this;
    }

    /**
     * Get idmarche
     *
     * @return integer 
     */
    public function getIdmarche()
    {
        return $this->idmarche;
    }

    /**
     * Set jour
     *
     * @param string $jour
     * @return Tbtiragejour
     */
    public function setJour($jour)
    {
        $this->jour = $jour;

        return $this;
    }

    /**
     * Get jour
     *
     * @return string 
     */
    public function getJour()
    {
        return $this->jour;
    }

    /**
     * Set datecreation
     *
     * @param \DateTime $datecreation
     * @return Tbtiragejour
     */
    public function setDatecreation($datecreation)
    {
        $this->datecreation = $datecreation;

        return $this;
    }

    /**
     * Get datecreation
     *
     * @return \DateTime 
     */
    public function getDatecreation()
    {
        return $this->datecreation;
    }

    /**
     * Set datemodification
     *
     * @param \DateTime $datemodification
     * @return Tbtiragejour
     */
    public function setDatemodification($datemodification)
    {
        $this->datemodification = $datemodification;

        return $this;
    }

    /**
     * Get datemodification
     *
     * @return \DateTime 
     */
    public function getDatemodification()
    {
        return $this->datemodification;
    }

    /**
     * Get idtbtiragejour
     *
     * @return integer 
     */
    public function getIdtbtiragejour()
    {
        return $this->idtbtiragejour;
    }
}
